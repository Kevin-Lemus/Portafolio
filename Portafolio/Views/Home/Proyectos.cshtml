@model IEnumerable<Proyecto>

@{
    ViewData["Title"] = "Proyectos";
}

@if (Model is not null)
{
    <div class="text-center" style="padding-top: 6rem;">
        <h1>Mis Proyectos</h1>
        <p>Aquí encontraras todos mis proyectos.</p>
    </div>

    <partial name="_ListadoProyectos" model="Model" />
}